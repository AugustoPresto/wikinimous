<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <% @articles.each do |article| %>
        <article>
          <%= link_to "#{article.title}", article_path(article) %>
          <p> <%= article.content %></p>
        </article>
      <% end %>
    </div>
  </div>
</div>
